// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+f",
        "command": "workbench.action.findInFiles"
    },
    {
        "key": "shift+cmd+f",
        "command": "-workbench.action.findInFiles"
    },
    {
        "key": "cmd+h",
        "command": "workbench.action.toggleMultiCursorModifier"
    },
    {
        "key": "ctrl+x ctrl+f",
        "command": "filesExplorer.findInWorkspace"
    },
    {
        "key": "ctrl+x ctrl+f",
        "command": "workbench.action.quickOpen"
    },
    {
        "key": "cmd+p",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "shift+cmd+\\",
        "command": "workbench.action.splitEditor"
    },
    {
        "key": "cmd+\\",
        "command": "-workbench.action.splitEditor"
    },
    {
        "key": "shift+cmd+,",
        "command": "workbench.action.closeSidebar"
    },
    {
        "key": "shift+cmd+p",
        "command": "workbench.action.showCommands"
    },
    {
        "key": "cmd+g",
        "command": "workbench.action.tasks.runTask",
        "args": "magit-status"
    },
    {
        "key": "cmd+l",
        "command": "workbench.action.gotoLine"
    },
    {
        "key": "ctrl+g",
        "command": "-workbench.action.gotoLine"
    },
    {
        "key": "cmd+w",
        "command": "workbench.action.closeWindow"
    },
    {
        "key": "shift+cmd+w",
        "command": "-workbench.action.closeWindow"
    },
    {
        "key": "shift+cmd+w",
        "command": "breadcrumbs.focusAndSelect",
        "when": "breadcrumbsPossible"
    },
    {
        "key": "shift+cmd+.",
        "command": "-breadcrumbs.focusAndSelect",
        "when": "breadcrumbsPossible"
    },
    {
        "key": "shift+cmd+w",
        "command": "breadcrumbs.toggleToOn",
        "when": "!config.breadcrumbs.enabled"
    },
    {
        "key": "shift+cmd+.",
        "command": "-breadcrumbs.toggleToOn",
        "when": "!config.breadcrumbs.enabled"
    },
    {
        "key": "shift+cmd+.",
        "command": "workbench.view.extensions",
        "when": "viewContainer.workbench.view.extensions.enabled"
    },
    {
        "key": "shift+cmd+x",
        "command": "-workbench.view.extensions",
        "when": "viewContainer.workbench.view.extensions.enabled"
    },
    {
        "key": "shift+cmd+x",
        "command": "python.execInTerminal"
    },
    {
        "key": "shift+cmd+=",
        "command": "workbench.action.zoomIn"
    },
    {
        "key": "cmd+numpad_add",
        "command": "-workbench.action.zoomIn"
    },
    {
        "key": "shift+cmd+-",
        "command": "workbench.action.splitEditorDown"
    },
    {
        "key": "cmd+w",
        "command": "workbench.action.closeActiveEditor"
    },
    {
        "key": "cmd+w",
        "command": "-workbench.action.closeActiveEditor"
    },
    {
        "key": "cmd+w cmd+w",
        "command": "workbench.action.closeGroup",
        "when": "activeEditorGroupEmpty && multipleEditorGroups"
    },
    {
        "key": "cmd+w",
        "command": "-workbench.action.closeGroup",
        "when": "activeEditorGroupEmpty && multipleEditorGroups"
    },
    {
        "key": "shift+cmd+o shift+cmd+o",
        "command": "workbench.action.reopenClosedEditor"
    },
    {
        "key": "shift+cmd+t",
        "command": "-workbench.action.reopenClosedEditor"
    },
    {
        "key": "cmd+alt+e",
        "command": "workbench.output.action.clearOutput"
    },
    {
        "key": "cmd+down",
        "command": "workbench.action.closePanel"
    },
    {
        "key": "cmd+up",
        "command": "terminal.focus"
    },
    {
        "key": "cmd+up",
        "command": "workbench.action.toggleMaximizedPanel",
        "when": "panelFocus"
    },
    {
        "key": "cmd+alt+left",
        "command": "workbench.panel.repl.view.focus",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+alt+left",
        "command": "workbench.panel.output.focus",
        "when": "inDebugRepl"
    },
    {
        "key": "cmd+alt+left",
        "command": "workbench.action.problems.focus",
        "when": "inOutput"
    },
    {
        "key": "cmd+alt+right",
        "command": "workbench.panel.output.focus",
        "when": "problemFocus"
    },
    {
        "key": "cmd+alt+right",
        "command": "workbench.panel.repl.view.focus",
        "when": "inOutput"
    },
    {
        "key": "cmd+alt+right",
        "command": "terminal.focus",
        "when": "inDebugRepl"
    },
    {
        "key": "cmd+alt+left",
        "command": "workbench.action.navigateBack",
        "when": "!panelFocus"
    },
    {
        "key": "cmd+alt+right",
        "command": "workbench.action.navigateForward",
        "when": "!panelFocus"
    },
    {
        "key": "cmd+r cmd+t",
        "command": "python.selectAndRunTestMethod"
    },
    {
        "key": "cmd+r cmd+t",
        "command": "testing.runAtCursor"
    },
    {
        "key": "cmd+d cmd+t",
        "command": "testing.debugAtCursor"
    },
    // Search result navigation (default bindings: f4 and shift+f4)
    {
        "key": "ctrl+down",
        "command": "references-view.next",
        "when": "reference-list.hasResult && references-view.canNavigate"
    },
    {
        "key": "ctrl+down",
        "command": "search.action.focusNextSearchResult",
        "when": "hasSearchResult || inSearchEditor"
    },
    {
        "key": "ctrl+down",
        "command": "goToNextReference",
        "when": "inReferenceSearchEditor || referenceSearchVisible"
    },
    {
        "key": "ctrl+up",
        "command": "references-view.prev",
        "when": "reference-list.hasResult && references-view.canNavigate"
    },
    {
        "key": "ctrl+up",
        "command": "search.action.focusPreviousSearchResult",
        "when": "hasSearchResult || inSearchEditor"
    },
    {
        "key": "ctrl+up",
        "command": "goToPreviousReference",
        "when": "inReferenceSearchEditor || referenceSearchVisible"
    },
    {
        "key": "alt+.",
        "command": "editor.action.revealDefinition",
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
]